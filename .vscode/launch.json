{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Execute Command",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "${workspaceFolder}/bin/dev.js",
      "runtimeArgs": ["hello", "-f dave", "abbie"],
//      "program": "${workspaceFolder}/bin/dev.js",
//      "args": ["hello", "world"]
    }
  ]
}
